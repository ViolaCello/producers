<%= form_with model: @user, local: true do |f|%>
  <%= f.label :name%><br>
  <%= f.text_field :name%><br>
<%= f.label :email%><br>
  <%= f.email_field :email%><br>
  <%= f.label :password%><br>
  <%= f.password_field :password%><br>
  <%= f.label :company%><br>
 <%= f.text_field :company%><br>

  <%= f.submit %>
<% end %>



