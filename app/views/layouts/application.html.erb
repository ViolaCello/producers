<!DOCTYPE html>
<html>
  <head>
    <title>Producers</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>





  </head>

  <body>
  <% if logged_in? %>
  <p>Logged in as: <%= current_user.name %></p>
  <p><%= link_to "Edit Functions", edit_user_path(current_user) %> </p>
  <p><%= link_to "logout", {:controller=> "sessions", :action=> "destroy"}, :method => :delete%></p>

  

  <% end %>
  <center>
    <%= yield %>
    </center>
  </body>
</html>
